Algoritmo Arreglo_de_5_numeros
	Dimension numeros[5]
	numeros[0]<-5
	numeros[1]<-4
	numeros[2]<-3
	numeros[3]<-2
	numeros[4]<-1
	Escribir "Índice 0: ", numeros[0]
	Escribir "Índice 1: ", numeros[1]
	Escribir "Índice 2: ", numeros[2]
	Escribir "Índice 3: ", numeros[3]
	Escribir "Índice 4: ", numeros[4]
	Para i<-0 Hasta 4 Con Paso 1
		Escribir "Índice ",i," contiene: ",numeros[i]
	FinPara	
FinAlgoritmo
